	pinctrl-names = "default";
	pinctrl-0 = <&uart1_pins>;
	status = "okay";

	/* Airoha AG3352 GPS */
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart2_pins>;
