	st7789v: st7789v@0 {
		compatible = "sitronix,st7789v";
		reg = <0>;
		status = "okay";
		spi-max-frequency = <48000000>;
		spi-cpol;
		spi-cpha;
		rotate = <0>;
		fps = <60>;
		buswidth = <8>;
		rgb;
		dc = <&pio 29 GPIO_ACTIVE_HIGH>;
		reset = <&pio 62 GPIO_ACTIVE_HIGH>;
		width = <240>;
		height = <240>;
		debug = <0x0>;
	};
*/
};

&ssusb0 {
	status = "okay";
};

&ssusb1 {
	status = "okay";
};

&switch {
	status = "okay";

	ports {
		port@0 {
			status = "disabled";
		};

		port@1 {
			status = "disabled";
		};

		port@2 {
			status = "disabled";
		};

		port@3 {
			label = "management";
		};
	};
};

&tphy {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&xphy {
	status = "okay";
};
