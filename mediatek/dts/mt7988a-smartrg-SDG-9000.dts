/dts-v1/;
#include "mt7988a.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/mt65xx.h>
#include <dt-bindings/regulator/richtek,rt5190a-regulator.h>

/ {
	model = "SmartRG SDG-9000";
	compatible = "smartrg,sdg-9000", "mediatek,mt7988a";

	aliases {
		serial0 = &uart0;
		ethernet0 = &gmac0;
		led-boot = &sys_status;
		led-failsafe = &sys_status;
		led-running = &sys_status;
		led-upgrade = &sys_status;
	};

	chosen {
		stdout-path = &uart0;
		bootargs = "console=ttyS0,115200n1 loglevel=8 pci=pcie_bus_perf";
	};

	memory {
		reg = <0x0 0x40000000 0x0 0x40000000>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/delete-node/ramoops@42ff0000;

		bootdata_reserved: bootdata@45000000 {
			no-map;
			reg = <0x0 0x45000000 0x0 0x00001000>;
		};

		ramoops_reserved: ramoops@45001000 {
			no-map;
			compatible = "ramoops";
			reg = <0x0 0x45001000 0x0 0x00140000>;
			ftrace-size = <0x20000>;
			record-size = <0x20000>;
			console-size = <0x20000>;
			pmsg-size = <0x80000>;
		};
	};

	bootdata {
		compatible = "bootdata";
		memory-region = <&bootdata_reserved>;
	};

	reg_1p8v: regulator-1p8v {
		compatible = "regulator-fixed";
		regulator-name = "fixed-1.8V";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-boot-on;
		regulator-always-on;
	};

	reg_3p3v: regulator-3p3v {
		compatible = "regulator-fixed";
		regulator-name = "fixed-3.3V";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		regulator-always-on;
	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&button_pins>;

		factory {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&pio 13 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-export {
		compatible = "gpio-export";

		pse0_reset {
			gpio-export,name = "pse0-reset";
			gpio-export,output = <1>;
			gpios = <&pio 24 GPIO_ACTIVE_LOW>;
		};

		pse1_reset {
			gpio-export,name = "pse1-reset";
			gpio-export,output = <1>;
			gpios = <&pio 26 GPIO_ACTIVE_LOW>;
		};
	};

	leds: gpio-leds {
		compatible = "gpio-leds";

		mgmt_amber {
			color = <LED_COLOR_ID_AMBER>;
			function = "mgmt";
			gpios = <&pio 65 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "mt7530-0:03:100Mbps";
		};

		mgmt_green {
			color = <LED_COLOR_ID_GREEN>;
			function = "mgmt";
			gpios = <&pio 64 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "mt7530-0:03:1Gbps";
		};

		sfp_green {
			color = <LED_COLOR_ID_GREEN>;
			function = "sfp";
			gpios = <&pio 67 GPIO_ACTIVE_LOW>;
		};

		sfp_red {
			color = <LED_COLOR_ID_RED>;
			function = "sfp";
			gpios = <&pio 66 GPIO_ACTIVE_HIGH>;
		};
	};

	sfp1: sfp1 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1>;
		los-gpios = <&pio 81 GPIO_ACTIVE_HIGH>;
		mod-def0-gpios = <&pio 82 GPIO_ACTIVE_LOW>;
		rate-select0-gpios = <&pio 83 GPIO_ACTIVE_HIGH>;
		rate-select1-gpios = <&pio 80 GPIO_ACTIVE_HIGH>;
		tx-disable-gpios = <&pio 36 GPIO_ACTIVE_HIGH>;
		tx-fault-gpios = <&pio 37 GPIO_ACTIVE_HIGH>;
		maximum-power-milliwatt = <4000>;
	};
};

&cpu0 {
	proc-supply = <&rt5190_buck3>;
};

&cpu1 {
	proc-supply = <&rt5190_buck3>;
};

&cpu2 {
	proc-supply = <&rt5190_buck3>;
};

&cpu3 {
	proc-supply = <&rt5190_buck3>;
};

&cci {
	proc-supply = <&rt5190_buck3>;
};

&cpu_thermal {
	/delete-node/cooling-maps;
	/delete-node/trips;

	trips {
		cpu_trip_crit: crit {
			temperature = <125000>;
			hysteresis = <2000>;
			type = "critical";
		};

		cpu_trip_hot: hot {
			temperature = <120000>;
			hysteresis = <2000>;
			type = "hot";
		};

		cpu_trip_active_high: active-high {
			temperature = <110000>;
			hysteresis = <2000>;
			type = "active";
		};

		cpu_trip_active_med: active-med {
			temperature = <80000>;
			hysteresis = <2000>;
			type = "active";
		};

		cpu_trip_active_low: active-low {
			temperature = <60000>;
			hysteresis = <2000>;
			type = "active";
		};

		cpu_trip_active_silent: active-silent {
			temperature = <40000>;
			hysteresis = <2000>;
			type = "active";
		};
	};

	cooling-maps {
		cpu-active-high {
			/* active: set fan to cooling level 3 */
			cooling-device = <&fan 3 3>;
			trip = <&cpu_trip_active_high>;
		};

		cpu-active-med {
			/* active: set fan to cooling level 2 */
			cooling-device = <&fan 2 2>;
			trip = <&cpu_trip_active_med>;
		};

		cpu-active-low {
			/* active: set fan to cooling level 1 */
			cooling-device = <&fan 1 1>;
			trip = <&cpu_trip_active_low>;
		};

		cpu-active-silent {
			/* active: set fan to cooling level 0 */
			cooling-device = <&fan 0 0>;
			trip = <&cpu_trip_active_silent>;
		};
	};
};

&fan {
	/* FIXME: add  support for 2nd fan */
	pwms = <&pwm 0 40000 0>;

	/**
	 * set fan speed
	 *
	 * 0 = off
	 * 61 = 24% duty cycle
	 * 77 = 30% duty cycle
	 * 102 = 40% duty cycle
	 * 128 - 50% duty cycle
	 * 255 = 100% duty cycle
	 */
	cooling-levels = <61 77 102 128>;

	interrupt-parent = <&pio>;
	interrupts = <51 IRQ_TYPE_EDGE_FALLING>;
	pulses-per-revolution = <2>;

	status = "okay";
};

&gmac0 {
	label = "dsa0";
	status = "okay";
};

&gmac1 {
	label = "dsa1";
	status = "okay";
	phy-mode = "10gbase-r";

	fixed-link {
		speed = <10000>;
		full-duplex;
		pause;
	};
};

&gmac2 {
	label = "wan";
	status = "okay";
	phy-mode = "usxgmii";
	phy-connection-type = "usxgmii";
	phy = <&phy0>;
};

&eth {
	pinctrl-0 = <&mdio0_pins>;
	pinctrl-names = "default";
	status = "okay";

	mux: mux-bus {
		mux2: ethernet-mux@2 {
			compatible = "mediatek,eth-mux";
			reg = <2>;
			label = "wan";

			chan-sel-gpios = <&pio 53 GPIO_ACTIVE_HIGH>;
			mod-def0-gpios = <&pio 82 GPIO_ACTIVE_LOW>;

			channel0: channel@0 {
				reg = <0>;
				phy-mode = "usxgmii";
				phy = <&phy0>;
			};

			channel1: channel@1 {
				reg = <1>;
				phy-mode = "usxgmii";
				sfp = <&sfp1>;
				managed = "in-band-status";
			};
		};
	};
};

&gsw_phy0 {
	status = "disabled";
};

&gsw_phy1 {
	status = "disabled";
};

&gsw_phy2 {
	status = "disabled";
};

&i2c0 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c0_pins>;
	status = "okay";

	rt5190a_64: rt5190a@64 {
		compatible = "richtek,rt5190a";
		reg = <0x64>;
		/*interrupts-extended = <&gpio26 0 IRQ_TYPE_LEVEL_LOW>;*/
		vin2-supply = <&rt5190_buck1>;
		vin3-supply = <&rt5190_buck1>;
		vin4-supply = <&rt5190_buck1>;

		regulators {
			rt5190_buck1: buck1 {
				regulator-name = "rt5190a-buck1";
				regulator-min-microvolt = <5090000>;
				regulator-max-microvolt = <5090000>;
				regulator-allowed-modes =
				<RT5190A_OPMODE_AUTO RT5190A_OPMODE_FPWM>;
				regulator-boot-on;
				regulator-always-on;
			};
			buck2 {
				regulator-name = "vcore";
				regulator-min-microvolt = <600000>;
				regulator-max-microvolt = <1400000>;
				regulator-boot-on;
				regulator-always-on;
			};
			rt5190_buck3: buck3 {
				regulator-name = "vproc";
				regulator-min-microvolt = <600000>;
				regulator-max-microvolt = <1400000>;
				regulator-boot-on;
				regulator-always-on; /* FIXME: need this for some reason to keep regulator on */
			};
			buck4 {
				regulator-name = "rt5190a-buck4";
				regulator-min-microvolt = <850000>;
				regulator-max-microvolt = <850000>;
				regulator-allowed-modes =
				<RT5190A_OPMODE_AUTO RT5190A_OPMODE_FPWM>;
				regulator-boot-on;
				regulator-always-on;
			};
			ldo {
				regulator-name = "rt5190a-ldo";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <1200000>;
				regulator-boot-on;
				regulator-always-on;
			};
		};
	};
};

&i2c1 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c1_pins>;
	status = "okay";

	/**
	 * SI34742 PoE: 0x40
	 */
};

&i2c2 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c2_1_pins>;
	status = "okay";

/*
	cst816s: cst816s@15 {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "hynitron,cst816s";
		reg = <0x15>;
		interrupt-parent = <&pio>;
		interrupts = <34 IRQ_TYPE_EDGE_RISING>;
		reset-gpios = <&pio 33 GPIO_ACTIVE_LOW>;
		touchscreen-size-x = <240>;
		touchscreen-size-y = <240>;
	};

	exp0: tca9535@20 {
		compatible = "ti,tca9535";
		reg = <0x20>;
		#gpio-cells = <2>;
		interrupt-parent = <&pio>;
		interrupts = <54 IRQ_TYPE_LEVEL_LOW>;
	};

	exp1: tca9535@22 {
		compatible = "ti,tca9535";
		reg = <0x22>;
		#gpio-cells = <2>;
		interrupt-parent = <&pio>;
		interrupts = <32 IRQ_TYPE_LEVEL_LOW>;
	};
*/

	system-leds {
		compatible = "srg,sysled";
		reg = <0x30>;

		system_red {
			label = "red";
			reg = <1>;
		};

		sys_status: system_green {
			label = "green";
			reg = <2>;
			linux,default-trigger = "timer";
		};

		system_blue {
			label = "blue";
			reg = <3>;
		};

		system_white {
			label = "white";
			reg = <4>;
		};
	};
};

&mdio_bus {
	phy0: ethernet-phy@0 {
		/* CUX3410 */
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <0>;

		reset-gpios = <&pio 0 GPIO_ACTIVE_LOW>;
		reset-assert-us = <100000>;
		reset-deassert-us = <1000000>;

		marvell,mdi-cfg-order = <0>;
		aqr,led-config = <0x80 0x60 0xC000>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@0 {
				reg = <0>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_WHITE>;
			};

			led@1 {
				reg = <1>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_ORANGE>;
			};

			led@2 {
				reg = <2>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_GREEN>;
			};
		};
	};

	phy8: ethernet-phy@8 {
		/* CUX3410 */
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <8>;

		reset-gpios = <&pio 68 GPIO_ACTIVE_LOW>;
		reset-assert-us = <100000>;
		reset-deassert-us = <1000000>;

		marvell,mdi-cfg-order = <0>;
		aqr,led-config = <0x80 0x60 0xC000>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@0 {
				reg = <0>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_WHITE>;
			};

			led@1 {
				reg = <1>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_ORANGE>;
			};

			led@2 {
				reg = <2>;
				function = LED_FUNCTION_WAN;
				color = <LED_COLOR_ID_GREEN>;
			};
		};
	};

	switch@10 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "mxl,86282";
		reg = <0x10>;
		dsa,member = <1 0>;

		/* FIXME: use passthru mode for now */
		status = "disabled";

		reset-gpios = <&pio 1 GPIO_ACTIVE_LOW>;
		reset-assert-us = <100000>;
		reset-deassert-us = <1000000>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				label = "lan1";
				phy-handle = <&swphy0>;
				phy-mode = "internal";
				status = "okay";
			};

			port@1 {
				reg = <1>;
				label = "lan2";
				phy-handle = <&swphy1>;
				phy-mode = "internal";
				status = "okay";
			};

			port@2 {
				reg = <2>;
				label = "lan3";
				phy-handle = <&swphy2>;
				phy-mode = "internal";
				status = "okay";
			};

			port@3 {
				reg = <3>;
				label = "lan4";
				phy-handle = <&swphy3>;
				phy-mode = "internal";
				status = "okay";
			};

			port@4 {
				reg = <4>;
				label = "lan5";
				phy-handle = <&swphy4>;
				phy-mode = "internal";
				status = "okay";
			};

			port@5 {
				reg = <5>;
				label = "lan6";
				phy-handle = <&swphy5>;
				phy-mode = "internal";
				status = "okay";
			};

			port@6 {
				reg = <6>;
				label = "lan7";
				phy-handle = <&swphy6>;
				phy-mode = "internal";
				status = "okay";
			};

			port@7 {
				reg = <7>;
				label = "lan8";
				phy-handle = <&swphy7>;
				phy-mode = "internal";
				status = "okay";
			};

			port@8 {
				reg = <8>;
				label = "cpu";
				phy-mode = "usxgmii";
				ethernet = <&gmac1>;
				dsa-tag-protocol = "mxl862";
				/* dsa-tag-protocol = "mxl862_8021q"; */

				fixed-link {
					speed = <10000>;
					full-duplex;
				};
			};

			/*
			port@9 {
				reg = <9>;
				label = "lan9";
				phy-handle = <&phy8>;
				phy-mode = "usxgmii";
				status = "okay";
			};
			*/
		};

		mdio {
			#address-cells = <1>;
			#size-cells = <0>;

			swphy0: swphy@0 {
				reg = <0>;
			};
			swphy1: swphy@1 {
				reg = <1>;
			};
			swphy2: swphy@2 {
				reg = <2>;
			};
			swphy3: swphy@3 {
				reg = <3>;
			};
			swphy4: swphy@4 {
				reg = <4>;
			};
			swphy5: swphy@5 {
				reg = <5>;
			};
			swphy6: swphy@6 {
				reg = <6>;
			};
			swphy7: swphy@7 {
				reg = <7>;
			};
		};
	};
};

&mmc0 {
	pinctrl-names = "default", "state_uhs";
	pinctrl-0 = <&mmc0_pins_emmc_51>;
	pinctrl-1 = <&mmc0_pins_emmc_51>;
	bus-width = <8>;
	max-frequency = <200000000>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	/* mmc-hs400-1_8v; */
	/* hs400-ds-delay = <0x12814>; */
	vmmc-supply = <&reg_3p3v>;
	vqmmc-supply = <&reg_1p8v>;
	non-removable;
	no-sd;
	no-sdio;
	status = "okay";
};

&pcie0 {
	status = "disabled";
};

&pcie1 {
	status = "disabled";
};

&pcie2 {
	status = "disabled";
};

&pcie3 {
	status = "disabled";
};

&pio {
	button_pins: button-pins {
		pins = "GPIO_RESET";
		mediatek,pull-down-adv = <0>; /* bias-disable */
	};

/*
	mxl86282_reset: mxl86282-reset-hog {
		gpio-hog;
		gpios = <1 GPIO_ACTIVE_LOW>;
		line-name = "mxl86282-reset";
		output-high;
	};
*/

	mux_oe: mux-oe-hog {
		gpio-hog;
		gpios = <52 GPIO_ACTIVE_LOW>;
		line-name = "mux-oe";
		output-high;
	};

	pwm_pins: pwm-pins {
		mux {
			function = "pwm";
			groups = "pwm0", "pwm1";
		};
	};
};

&pwm {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm_pins>;
	status = "okay";
};

&spi1 {
	status = "okay";

